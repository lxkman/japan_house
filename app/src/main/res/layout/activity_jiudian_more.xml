<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/view_more_header"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"/>

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dip"
                android:orientation="horizontal">

                <View
                    android:layout_width="3dip"
                    android:layout_height="13dip"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="15dip"
                    android:background="@color/shihuangse"></View>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dip"
                    android:text="@string/string_jibenxinxi"
                    android:textColor="#444"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_shoujia"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_jiudian_shoujia"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_jidianloucengshu"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_jiudian_jiudianloucengshu"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_suoyouquan"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_jiudian_suoyouquan"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_chezhanjuli"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_jiudian_chezhanjuli"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_jianzunianfen"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_jiudian_jianzhunianfen"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_jianzugouzao"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_jiudian_jianzhugouzao"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_diyuyongtu"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_jiudian_diyuyongtu"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_xianzhuang"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_jiudian_xianzhuang"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_huibaolv"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_jiudian_huibaolv"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray1"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dip"
                android:orientation="horizontal">

                <View
                    android:layout_width="3dip"
                    android:layout_height="13dip"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="15dip"
                    android:background="@color/shihuangse"></View>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:paddingLeft="6dip"
                    android:text="@string/string_beizhuxinxi"
                    android:textColor="#444"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginBottom="6dip"
                android:layout_marginTop="6dip"
                android:paddingLeft="15dip"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="14sp"
                    android:text="@string/string_beizhu"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <TextView
                    android:id="@+id/act_gaoerfu_beizhu"
                    android:textColor="#222"
                    android:textSize="14sp"
                    android:text=""
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</LinearLayout>