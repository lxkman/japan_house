<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 填充的颜色 -->
    <solid android:color="#fff6f2"/>
    <!-- 设置按钮的四个角为弧形 -->
    <!-- android:radius 弧形的半径 -->
    <stroke android:color="#fff6f2" android:width="1dp"/>
    <corners android:radius="25dp"/>
    <padding
        android:bottom="8dp"
        android:left="20dp"
        android:right="20dp"
        android:top="8dp" />
</shape>